const int g1 = 3, g2 = 7, g3 = 11;

int main() {
    // 多阶段重新赋值
    int a = g1 * 2;               // 3 * 2=6
    a = a + g2 % 4;               // 6+3=9
    a = a * (g3 - 8);             // 9*(11-8)=27
    
    // 带const的中间计算
    const int b = (a / 3) + g2;   // 27/3=9 +7=16
    int c = b * 4;                // 16 * 4=64
    c = c - (g1 + 15);            // 64-18=46
    
    // 复合赋值操作
    int d = (g2 + 1) * (c % 6);   // 8 * 4=32
    d = d + (b % (g3 * 2)) * 2;   // 16%22=16 →32+32=64
    d = d / (g1 + 1);             // 64/4=16
    
    // 最终表达式
    return (a * (d%3 * 2) * c) - (b + g2 * 5);
}