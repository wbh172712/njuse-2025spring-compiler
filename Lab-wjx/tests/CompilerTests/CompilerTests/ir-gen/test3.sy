int main() {
    const int kMax = 0x6;
    int result = 0, i = 0;
    while (i < kMax) {
        int j = i;
        while (j < kMax) {
            if (j == kMax - 1) {
                break;
            }
            if (j % 2 == 0) {
                j = j + 1;
                continue;
            }
            result = result + 1;
            j = j + 01;
        }
        i = i + 1;
    }
    int a = 0;
    int b = 01;
    if (a != 0 && b != 0) {
        return - 0xffff;
    }
    return result;
}